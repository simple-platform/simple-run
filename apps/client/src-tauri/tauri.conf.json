{
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "mix phx.server",
    "devPath": "http://localhost:5000",
    "distDir": "http://localhost:5000"
  },
  "package": {
    "productName": "Simple Run",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "shell": {
        "scope": [
          {
            "args": [
              "start"
            ],
            "name": "../burrito_out/desktop",
            "sidecar": true
          }
        ],
        "sidecar": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [
        "../burrito_out/desktop"
      ],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "dev.simple.run",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Run containerized applications easily on your local machine.",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "title": "Simple Run",
        "width": 800,
        "fullscreen": false,
        "height": 600,
        "resizable": true
      }
    ]
  }
}
